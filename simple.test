#!/bin/bash
if [ $1 == "1" ]
then
    Rscript -e "library(annovarR);cat(download.database(show.all.names=T), sep = '\n')"
else
    Rscript -e "library('annovarR');download.database('$1', version = '$2',buildver = '$3', database.dir = sprintf('/tmp/test_BioInstaller/%s/%s', basename(tempdir()), '$2'))"
fi
